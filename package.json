{
  "name": "vue-split-pane",
  "version": "0.0.6",
  "description": "Adjustable split pane using Vue.js",
  "author": "Dang Van Thanh <dangvanthanh@dangthanh.org>",
  "main": "dist/vue-split-pane.js",
  "unpkg": "dist/vue-split-pane.min.js",
  "module": "dist/vue-split-pane.esm.js",
  "scripts": {
    "build": "npm run build:prod && npm run build:dev && npm run build:esm",
    "build:prod": "rollup -c rollup.config.prod.js",
    "build:dev": "rollup -c rollup.config.dev.js",
    "build:esm": "rollup -c rollup.config.esm.js",
    "release": "np"
  },
  "peerDependencies": {
    "vue": ">= 2"
  },
  "repository": {
    "type": "git",
    "url": "vue-split-pane"
  },
  "keywords": ["vue", "split-pane", "vue-split-pane"],
  "license": "MIT",
  "devDependencies": {
    "rollup": "^0.55.5",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.19.1",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-uglify": "^3.0.0",
    "rollup-plugin-vue": "^3.0.0",
    "uglify-es": "^3.3.10",
    "vue-template-compiler": "^2.5.13"
  }
}
